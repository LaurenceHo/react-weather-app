{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "arrow-parens": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "member-access": false,
    "no-console": false,
    "no-debugger": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "no-trailing-whitespace": false,
    "object-literal-sort-keys": false,
    "prefer-for-of": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false
  }
}
