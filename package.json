{
  "name": "react-weather-app",
  "version": "3.3.3",
  "description": "react-weather-app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --open",
    "build": "npm run lint && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "firebase-deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "firebase-add": "firebase use --add",
    "deploy-functions": "firebase deploy --only functions",
    "lint": "eslint --ext .ts,.tsx src --fix"
  },
  "author": "Laurence Ho",
  "license": "MIT",
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Typescript",
    "D3",
    "antd",
    "ECharts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.21.2",
    "connected-react-router": "^6.5.2",
    "d3": "^5.9.7",
    "echarts": "^4.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.1.10",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "firebase-tools": "^7.2.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 10"
  }
}
