{
  "name": "react-weather-app",
  "version": "3.4.3",
  "description": "React Weather App",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --open",
    "build": "npm run lint && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "firebase-deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "firebase-add": "firebase use --add",
    "deploy-functions": "firebase deploy --only functions",
    "lint": "eslint --ext .ts,.tsx src --fix"
  },
  "author": "Laurence Ho",
  "license": "MIT",
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Typescript",
    "D3",
    "antd",
    "ECharts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.26.8",
    "d3": "^5.15.0",
    "echarts": "^4.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.4.2",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "firebase-tools": "^7.12.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 10"
  }
}
