{
  "name": "react-weather-app",
  "version": "3.3.2",
  "description": "react-weather-app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --open",
    "build": "npm run lint && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "firebase-deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "firebase-add": "firebase use --add",
    "deploy-functions": "firebase deploy --only functions",
    "lint": "eslint --ext .ts,.tsx src --fix"
  },
  "author": "Laurence Ho",
  "license": "MIT",
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Typescript",
    "D3",
    "antd",
    "ECharts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "node_modules",
      "dist",
      "package-lock.json"
    ]
  },
  "dependencies": {
    "antd": "^3.19.3",
    "connected-react-router": "^6.4.0",
    "d3": "^5.9.2",
    "echarts": "^4.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.1.9",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "firebase-tools": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 8"
  }
}
