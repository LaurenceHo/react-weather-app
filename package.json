{
  "name": "react-weather-app",
  "version": "3.6.1",
  "description": "React Weather App",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --open",
    "build": "npm run lint && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "firebase-deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "firebase-add": "firebase use --add",
    "deploy-functions": "firebase deploy --only functions",
    "lint": "eslint --ext .ts,.tsx src --fix"
  },
  "author": "Laurence Ho",
  "license": "MIT",
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Typescript",
    "D3",
    "antd",
    "ECharts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.6.5",
    "clsx": "^1.1.1",
    "echarts": "^4.9.0",
    "firebase": "^7.21.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.6.5",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^3.6.0",
    "d3": "^5.16.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "firebase-tools": "^8.11.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.1.2",
    "source-map-loader": "^1.1.0",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 10"
  }
}
