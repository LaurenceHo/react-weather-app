{
  "name": "react-weather-app",
  "version": "3.4.3",
  "description": "React Weather App",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --progress --profile --watch --open",
    "build": "npm run lint && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "firebase-deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "firebase-add": "firebase use --add",
    "deploy-functions": "firebase deploy --only functions",
    "lint": "eslint --ext .ts,.tsx src --fix"
  },
  "author": "Laurence Ho",
  "license": "MIT",
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Typescript",
    "D3",
    "antd",
    "ECharts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.26.2",
    "d3": "^5.14.2",
    "echarts": "^4.5.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/echarts": "^4.4.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^4.3.0",
    "firebase-tools": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 10"
  }
}
